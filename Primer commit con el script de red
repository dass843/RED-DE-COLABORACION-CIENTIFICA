data <- rbind(data1920_2010,data2011_2015,data2016_2019,data2020_2022,data2023_2024)
dim(data)
names(data)

#COMO ES LA DATA DE AFILIACIONES 

data$Affiliations[1000]

#####
#### EXPANDIR AFILIACIONES Y EXTRAER NOMBRE DE UNIVERSIDAD ####
data_long <- data %>%
  filter(!is.na(Affiliations)) %>%
  separate_rows(Affiliations, sep = ";") %>%
  mutate(Affiliations = str_trim(Affiliations))

names(data_long)
data_long$Affiliations

library(dplyr)
library(stringr)
library(tidyr)

# Extraer universidad y país de las afiliaciones
# Versión para manejar múltiples afiliaciones por fila

data_processed <- data_long %>%
  filter(!is.na(Affiliations)) %>%
  # Separar afiliaciones individuales
  
  mutate(affiliation_list = str_split(Affiliations, ";")) %>%
  tidyr::unnest_longer(affiliation_list) %>%
  mutate(
    country = map_chr(str_split(affiliation_list, ","), ~ str_trim(tail(.x, 1))),
    university = map_chr(str_split(affiliation_list, ","), ~ str_trim(head(.x, 1)))
  ) %>%
  group_by(university, country) %>%
  mutate(university_id = cur_group_id()) %>%
  ungroup()

names(data_processed)
data_processed$university

library(dplyr)
library(stringr)

data_ready <- data_processed %>%
  select(
    university,
    Title,
    country,
    university_id,
    Year,
    Authors
  ) %>%
  distinct() %>%
  # Filtrar universidades que cumplan con los patrones
  filter(
    str_detect(
      university, 
      regex("^(Univ\\.?|Poli\\.?|Poly\\.?|University|Universidad|Polytechnic|Politecnico)|(Univ\\.?|Poli\\.?|Poly\\.?|University|Universidad|Polytechnic|Politecnico)$", ignore_case = TRUE)))

head(data_ready)
str(data_ready)
names(data_ready)
unique(data_ready$university[data_ready$country == 'Ecuador'])
#write.csv(data_ready,'C:/Users/ANDRES/Desktop/RedUni2/data_ready.csv')
#####

####LIMPIEZA TOTAL 95%####

# Guardar hora de inicio
start_time <- Sys.time()


library(dplyr)
library(stringr)
library(stringdist)
library(countrycode)
library(purrr)

# Cargar el dataset de referencia
world_univ <- read.csv("C:/Users/ANDRES/Desktop/RedUni2/world-universities1.csv", stringsAsFactors = FALSE) %>%
  distinct(name, country, .keep_all = TRUE) %>%
  mutate(
    country_full = countrycode(country, "iso2c", "country.name"),
    country_full = case_when(
      country_full == "United States" ~ "United States of America",
      country_full == "United Kingdom" ~ "United Kingdom of Great Britain and Northern Ireland",
      TRUE ~ country_full
    )
  )

# Diccionario multilingüe de términos universitarios
university_terms <- c(
  "university", "universität", "universiteit", "universidad", "université", "universita", 
  "polytechnic", "politecnico", "école", "escuela", "hochschule", "college", "institute",
  "instituto", "school", "academy", "akademie", "académie", "faculty", "faculteit",
  "univ", "ute", "epn", "espoch"  # Agregar siglas comunes
)

# Función avanzada de limpieza de nombres (multilingüe)
clean_university_name <- function(input_name) {
  cleaned <- input_name %>%
    # Eliminar contenido entre paréntesis y corchetes
    str_remove_all("\\(.*?\\)|\\[.*?\\]") %>%
    # Eliminar puntuación
    str_remove_all("[[:punct:]]") %>%
    # Convertir a minúsculas
    str_to_lower() %>%
    # Eliminar palabras irrelevantes en múltiples idiomas
    str_remove_all(paste0("\\b(", paste(university_terms, collapse = "|"), ")\\b")) %>%
    # Eliminar espacios extras
    str_squish()
  
  return(cleaned)
}

# Función para encontrar la mejor coincidencia por país
find_best_match <- function(input_name, input_country) {
  # Limpiar el nombre de entrada
  clean_input <- clean_university_name(input_name)
  
  # Normalizar el país de entrada
  norm_country <- case_when(
    input_country %in% c("United States", "USA", "US") ~ "United States of America",
    input_country %in% c("United Kingdom", "UK", "England") ~ "United Kingdom of Great Britain and Northern Ireland",
    input_country == "Russia" ~ "Russian Federation",
    input_country == "Brasil" ~ "Brazil",  # Corrección adicional
    TRUE ~ input_country
  )
  
  # Filtrar referencias por país
  country_ref <- world_univ %>% 
    filter(country_full == norm_country)
  
  if (nrow(country_ref) == 0) country_ref <- world_univ
  
  # Crear versiones limpias de los nombres de referencia
  country_ref_clean <- country_ref %>%
    mutate(clean_ref = clean_university_name(name))
  
  # Calcular distancias
  distances <- stringdist(
    clean_input,
    country_ref_clean$clean_ref,
    method = "jw",
    p = 0.1
  )
  
  # Encontrar la mejor coincidencia
  min_index <- which.min(distances)
  min_distance <- min(distances, na.rm = TRUE)
  
  # Aplicar umbral de calidad
  if (min_distance < 0.3) {
    return(country_ref$name[min_index])
  } else {
    return(NA)
  }
}

# Vectorizar la función
vectorized_matcher <- Vectorize(find_best_match, vectorize.args = c("input_name", "input_country"))

###Aplicar a todo el dataset
data_ready1 <- data_ready %>%
  mutate(
    normalized_university = vectorized_matcher(university, country)
  )


# Guardar hora de fin
end_time <- Sys.time()

# Calcular duración en segundos
duration <- as.numeric(difftime(end_time, start_time, units = "secs"))

# Convertir a minutos y segundos
minutos <- floor(duration / 60)
segundos <- round(duration %% 60)

cat("⏱ El proceso de limpieza del 95% demoró:", minutos, "minutos y", segundos, "segundos\n")


#7000
sort(unique(data_ready1$normalized_university[data_ready1$country == 'Ecuador']))



numero_na <- sum(is.na(data_ready1$normalized_university))

# Imprime el resultado
print(numero_na)

