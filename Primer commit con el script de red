for (case in global_cases) {
  data_ready1 <- data_ready1 %>%
    mutate(
      normalized_university = ifelse(
        str_detect(university, case$pattern) & country == case$country,
        case$replacement,
        normalized_university
      )
    )
}

#Lista universidades Ecuatorianas 
sort(unique(data_ready1$normalized_university[data_ready1$country == 'Ecuador']))

#Lista universidades internacionales

sort(unique(data_ready1$normalized_university[
  data_ready1$country != "Ecuador" &
    !is.na(data_ready1$normalized_university) &
    !is.na(data_ready1$country)
]))

#Numero de NA normalizados 
numero_na <- sum(is.na(data_ready1$normalized_university))

# Imprime el resultado
print(numero_na)

#####
#### Crear matriz ordenada alfabéticamente ####
str(data_ready1)
data_ready1 <- na.omit(data_ready1)#IMPORTANTE PARA ELIMINAR LOS NA
print(data_ready1)

head(data_ready1)
str(data_ready1)

##HACER LA RED TITULO X UNIVERSIDAD
matriz_UT <- data_ready1 %>%
  # 1. Filtrar y contar ocurrencias
  filter(!is.na(Title), 
         !is.na(normalized_university)) %>%
  count(Title, normalized_university) %>%
  
  # 2. Ordenar  (filas) y  (columnas) alfabéticamente   REVISAR NOMBRES 
  arrange(Title, normalized_university) %>%  # Ordenar filas y columnas
  pivot_wider(
    names_from = normalized_university,
    values_from = n,
    values_fill = list(n = 0),
    names_sort = TRUE  # Orden alfabético para columnas
  ) %>%
  
  # 3. Convertir a matriz
  tibble::column_to_rownames("Title") %>%
  as.matrix()

# Colapsar/proyectar la red sobre las columnas (tipo B)
matriz_U <- crossprod(matriz_UT)##RELACIONES UNIVERSIDAD

# Quitar la diagonal (no nos interesa la conexión de uno consigo mismo)
diag(matriz_U) <- 0

library(igraph)
library(tibble)
# Convertir a formato largo RED
matriz_larga <- as.data.frame(matriz_U) %>%
  rownames_to_column(var = "from") %>%
  pivot_longer(cols = -from, names_to = "to", values_to = "freq")
print(matriz_larga)


##### ELIMINAR CEROS MATRIZ LARGA FILTRADA  ########

# ELIMINAR CEROS
# matriz_larga_filtrada <- matriz_larga %>% filter(freq > 0)
matriz_larga_filtrada <- matriz_larga %>% filter(freq > 10)
print(matriz_larga_filtrada)


##### GRAFO DE FIGURA 1 #####


# con rangos y lollipop + rug ---

library(dplyr)
library(ggplot2)
library(scales)

# Parámetros

start_year <- 1990
end_year   <- 2024

# Datos agregados y filtrados

pubs_year <- data %>%
  filter(!is.na(Year),
         Year >= start_year, Year <= end_year) %>%
  count(Year, name = "publicaciones") %>%
  arrange(Year) %>%
  # Definimos rangos (ajústalos si quieres otros cortes)
  
  mutate(rango = cut(
    publicaciones,
    breaks = c(-Inf, 100, 500, 1000, 2500, 5000, Inf),
    labels = c("≤100", "101–500", "501–1,000", "1,001–2,500", "2,501–5,000", "≥5,001"),
    right = TRUE
  ))

# Paleta (más contrastes que el negro)

cols <- c("≤100"="#6BAED6", "101–500"="#3182BD", "501–1,000"="#31A354",
          "1,001–2,500"="#74C476", "2,501–5,000"="#FD8D3C", "≥5,001"="#E6550D")

# Eje X legible

tick_by   <- 2
tick_size <- 7
n_dodge   <- 3

ggplot(pubs_year, aes(x = Year, y = publicaciones)) +
  
  # "barras" de cada punto (lollipop stems)
  
  geom_segment(aes(xend = Year, y = 0, yend = publicaciones, colour = rango),
               linewidth = 0.6, alpha = 0.7) +
  
  # línea de tendencia anual
  
  geom_line(linewidth = 0.9, colour = "grey30") +
  
  # puntos coloreados por rango
  
  geom_point(aes(colour = rango), size = 2.2) +
  
  # suavizado opcional (quita si no lo quieres)
  
  geom_smooth(method = "loess", se = FALSE, span = 0.25,
              linetype = "dashed", colour = "grey40") +
  
  # “barras laterales” que muestran la distribución de valores
  
  geom_rug(aes(colour = rango), sides = "r", alpha = 0.6, linewidth = 0.6) +
  scale_color_manual(values = cols, name = "Rango de publicaciones") +
  scale_y_continuous(labels = label_number(big.mark = ",")) +
  scale_x_continuous(
    limits = c(start_year, end_year),
    breaks  = seq(start_year, end_year, by = tick_by),
    guide   = guide_axis(n.dodge = n_dodge)
  ) +
  labs(
    title = "Publicaciones por año (1990–2024)",
    x = "Año",
    y = "N° de publicaciones"
  ) +
  theme_minimal(base_size = 11) +
  theme(
    axis.text.x = element_text(size = tick_size), # si prefieres: angle = 45, hjust = 1
    plot.title  = element_text(face = "bold")
  )




####


##### GRAFO FIGURA 2 ####

library(dplyr)
library(tidyr)
library(stringr)

# 1. Todas las universidades desde edges
universidades_todas <- matriz_larga_filtrada %>%
  select(from, to) %>%
  pivot_longer(cols = everything(), values_to = "label") %>%
  mutate(label = str_trim(label)) %>%        # quitar espacios
  distinct(label)

# 2. Crear nodos con country desde data_ready
nodos <- universidades_todas %>%
  left_join(
    data_ready1 %>%
      mutate(normalized_university = str_trim(normalized_university)) %>%
      arrange(normalized_university, Year) %>%
      group_by(normalized_university) %>%
      slice_head(n = 1) %>%
      ungroup() %>%
      select(label = normalized_university, country),
    by = "label"
  ) %>%
  mutate(id = row_number())

# 3. Filtrar edges para que solo usen nodos presentes
matriz_filtrada_edges <- matriz_larga_filtrada %>%
  mutate(from = str_trim(from),
         to   = str_trim(to)) %>%
  filter(from %in% nodos$label & to %in% nodos$label)
edges_clean <- matriz_filtrada_edges %>%
  mutate(n1 = pmin(from, to),
         n2 = pmax(from, to)) %>%    # asegura orden único
  group_by(n1, n2) %>%
  summarise(freq = sum(freq), .groups = "drop") %>%
  rename(from = n1, to = n2)

# 4. Crear grafo
grafo <- graph_from_data_frame(
  d = edges_clean,
  directed = FALSE,
  vertices = nodos
)

#V(grafo)$name
#V(grafo)$country
#E(grafo)$freq



library(igraph)

# --- Definir colores y formas ---
vertex_colors <- ifelse(V(grafo)$country == "Ecuador", "darkgreen", "steelblue")
vertex_shapes <- ifelse(V(grafo)$country == "Ecuador", "square", "circle")

# --- Etiquetas: solo mostrar nombres de universidades ecuatorianas ---
vertex_labels <- ifelse(V(grafo)$country == "Ecuador", V(grafo)$name, "")

# --- Excepción: Universidad Católica de Cuenca ---
ucc <- which(V(grafo)$name == "Universidad Católica de Cuenca")

vertex_colors[ucc] <- "red"       # vértice en rojo
vertex_shapes[ucc] <- "square"    # mantener cuadrado
vertex_label_colors <- ifelse(V(grafo)$name == "Universidad Católica de Cuenca", 
                              "red", "darkblue")   # nombre en rojo solo para UCC

# --- Plotear ---
par(mar = c(0,0,0,0))
set.seed(1001)
plot(grafo,
     vertex.label = vertex_labels,
     vertex.label.color = vertex_label_colors,
     edge.width = log10(E(grafo)$freq + 1),   # evitar log(0)
     layout = layout_with_fr,
     vertex.color = vertex_colors,
     vertex.frame.color = vertex_colors,
     vertex.size = log(degree(grafo) + 1),
     vertex.label.cex = 0.6,
     vertex.shape = vertex_shapes,
     main = "")


legend("bottomleft",                 # Posición de la leyenda
       legend = c('Internacional',
                  'Ecuador'),        # Etiquetas de los grupos
       pch = c(19,15),                      # Símbolo de la leyenda (círculo relleno)
       col = c( "steelblue", "darkgreen"),         # Colores correspondientes
       title = "",              # Título de la leyenda
       pt.cex = 2,                 # Tamaño de los símbolos
       cex = 1,                    # Tamaño del texto
       bty = "n")                    # Eliminar el cuadro alrededor



####



#### GRAFO DE LA FIGURA 3 ####

# ELIMINAR CEROS
matriz_larga_filtrada <- matriz_larga %>% filter(freq > 0)

print(matriz_larga_filtrada)


library(dplyr)
library(tidyr)
library(stringr)

# 1. Todas las universidades desde edges
universidades_todas <- matriz_larga_filtrada %>%
  select(from, to) %>%
  pivot_longer(cols = everything(), values_to = "label") %>%
  mutate(label = str_trim(label)) %>%        # quitar espacios
  distinct(label)

# 2. Crear nodos con country desde data_ready
nodos <- universidades_todas %>%
  left_join(
    data_ready1 %>%
      mutate(normalized_university = str_trim(normalized_university)) %>%
      arrange(normalized_university, Year) %>%
      group_by(normalized_university) %>%
      slice_head(n = 1) %>%
      ungroup() %>%
      select(label = normalized_university, country),
    by = "label"
  ) %>%
  mutate(id = row_number())

# 3. Filtrar edges para que solo usen nodos presentes
matriz_filtrada_edges <- matriz_larga_filtrada %>%
  mutate(from = str_trim(from),
         to   = str_trim(to)) %>%
  filter(from %in% nodos$label & to %in% nodos$label)
edges_clean <- matriz_filtrada_edges %>%
  mutate(n1 = pmin(from, to),
         n2 = pmax(from, to)) %>%    # asegura orden único
  group_by(n1, n2) %>%
  summarise(freq = sum(freq), .groups = "drop") %>%
  rename(from = n1, to = n2)

# 4. Crear grafo
grafo <- graph_from_data_frame(
  d = edges_clean,
  directed = FALSE,
  vertices = nodos
)

E(grafo)$freq
E(grafo)$freq
V(grafo)$name
V(grafo)$country

library(igraph)

# Filtrar solo universidades de Ecuador
ecuador_nodes <- V(grafo)[V(grafo)$country == "Ecuador"]
subgrafo_ecuador <- induced_subgraph(grafo, vids = ecuador_nodes)

# Definir colores y formas
V(subgrafo_ecuador)$color <- "darkgreen"        # nodos de Ecuador verdes
V(subgrafo_ecuador)$frame.color <- "darkgreen"
V(subgrafo_ecuador)$shape <- "square"

# Crear vectores de color y fuente para etiquetas
vertex_label_colors <- rep("darkblue", vcount(subgrafo_ecuador))  # etiquetas negras por defecto
vertex_label_fonts  <- rep(1, vcount(subgrafo_ecuador))       # fuente normal

# Cambiar solo Universidad Católica de Cuenca
target <- which(V(subgrafo_ecuador)$name == "Universidad Católica de Cuenca")
V(subgrafo_ecuador)$color[target] <- "red"          # nodo en rojo
V(subgrafo_ecuador)$frame.color[target] <- "red"    # borde en rojo
vertex_label_colors[target] <- "red"                # etiqueta en rojo
vertex_label_fonts[target] <- 2                     # etiqueta en negrita

# Plotear
par(mar = c(0,0,0,0))
set.seed(1001)
plot(subgrafo_ecuador,
     vertex.label = V(subgrafo_ecuador)$name,
     vertex.label.color = vertex_label_colors,
     vertex.label.font = vertex_label_fonts,
     edge.width = log10(E(grafo)$freq + 1),  # evita log(0)
     layout = layout_with_fr,
     vertex.size = log(degree(subgrafo_ecuador) + 1),
     vertex.shape = V(subgrafo_ecuador)$shape,
     vertex.color = V(subgrafo_ecuador)$color,
     vertex.frame.color = V(subgrafo_ecuador)$frame.color,
     vertex.label.cex = 0.6,
     main = "")



####


##### GRAFO DE FIGURA 4 #####

library(igraph)

# --- 1. Nodo central ---
central_node <- V(grafo)[V(grafo)$name == "Universidad Católica de Cuenca"]

# --- 2. Identificar vecinos (conectadas a la central) ---
vecinos <- neighbors(grafo, v = central_node)

# --- 3. Crear subgrafo: central + vecinos ---
subgrafo_cuenca <- induced_subgraph(grafo, vids = union(central_node, vecinos))

# --- 4. Colores y formas ---
V(subgrafo_cuenca)$color <- ifelse(
  V(subgrafo_cuenca)$name == "Universidad Católica de Cuenca", "darkred",          # central en rojo
  ifelse(V(subgrafo_cuenca)$country == "Ecuador", "darkgreen", "steelblue")       # Ecuador verde, internacionales azul
)
V(subgrafo_cuenca)$frame.color <- V(subgrafo_cuenca)$color

V(subgrafo_cuenca)$shape <- ifelse(
  V(subgrafo_cuenca)$name == "Universidad Católica de Cuenca", "square",           # central cuadrado
  ifelse(V(subgrafo_cuenca)$country == "Ecuador", "square", "circle")              # Ecuador cuadrados, internacionales círculos
)

# --- 5. Etiquetas: solo nodo central ---
vertex_labels <- ifelse(V(subgrafo_cuenca)$name == "Universidad Católica de Cuenca",
                        V(subgrafo_cuenca)$name, "")

# --- 6. Plotear ---
par(mar = c(0,0,0,0))
set.seed(1001)
plot(subgrafo_cuenca,
     vertex.label = vertex_labels,
     vertex.label.color = 'red',
     edge.width = log10(E(grafo)$freq + 1),  # evitar log(0)
     layout = layout_with_fr,
     vertex.size = log(degree(subgrafo_cuenca) + 1),
     vertex.label.cex = 1,
     vertex.shape = V(subgrafo_cuenca)$shape,
     main = "")
legend("bottomleft",                 # Posición de la leyenda
       legend = c('Internacional',
                  'Ecuador'),        # Etiquetas de los grupos
       pch = c(19,15),                      # Símbolo de la leyenda (círculo relleno)
       col = c( "steelblue", "darkgreen"),         # Colores correspondientes
       title = "",              # Título de la leyenda
       pt.cex = 2,                 # Tamaño de los símbolos
       cex = 1,                    # Tamaño del texto
       bty = "n")                    # Eliminar el cuadro alrededor



####



##### GRAFO DE FIGURA 5 #####


library(dplyr)
library(igraph)

ucc_name <- "Universidad Católica de Cuenca"

# ===== 1) Grados y Top 6 ===
deg_all <- degree(grafo)
df_deg <- data.frame(
  name    = V(grafo)$name,
  country = V(grafo)$country,
  degree  = as.integer(deg_all),
  stringsAsFactors = FALSE
)

top6_ecu <- df_deg %>% filter(country == "Ecuador") %>% arrange(desc(degree)) %>% slice_head(n = 6)
top6_int <- df_deg %>% filter(country != "Ecuador", !is.na(country)) %>% arrange(desc(degree)) %>% slice_head(n = 6)

# ===== 2) Subgrafo con Top6 EC + Top6 INT (+ UCC si aplica) ===
nodes_interest <- unique(c(top6_ecu$name, top6_int$name))
if (ucc_name %in% V(grafo)$name && !(ucc_name %in% nodes_interest)) {
  nodes_interest <- c(nodes_interest, ucc_name)
}
subg <- induced_subgraph(grafo, vids = V(grafo)[name %in% nodes_interest])

# ===== 3) Atributos visuales ===
V(subg)$color <- ifelse(V(subg)$country == "Ecuador", "darkgreen", "steelblue")
V(subg)$shape <- ifelse(V(subg)$country == "Ecuador", "square", "circle")
V(subg)$frame.color <- V(subg)$color

# Resaltar UCC
if (ucc_name %in% V(subg)$name) {
  ucc_vid <- which(V(subg)$name == ucc_name)
  V(subg)$color[ucc_vid]       <- "red"
  V(subg)$frame.color[ucc_vid] <- "red"
  V(subg)$shape[ucc_vid]       <- "square"
}

# Tamaño por grado dentro del subgrafo (sin mostrar números)
deg_sub <- degree(subg)
V(subg)$size <- pmax(6, 2 + 1.2 * log(deg_sub + 1))

# ===== 4) Plot (solo pesos en aristas) ===
par(mar = c(0,0,2,0))
set.seed(123)
L <- layout_with_fr(subg)

plot(subg,
     layout             = L,
     vertex.color       = V(subg)$color,
     vertex.shape       = V(subg)$shape,
     vertex.frame.color = V(subg)$frame.color,
     vertex.size        = V(subg)$size,
     vertex.label       = V(subg)$name,     # solo nombres
     vertex.label.color = "black",
     vertex.label.cex   = 0.75,
     edge.width         = log10(E(subg)$freq + 1),
     edge.label         = E(subg)$freq,     # ← pesos en aristas
     edge.label.cex     = 0.75,
     edge.label.color   = "gray20",
     main               = "Top 6 Ecuador + Top 6 Internacional\n(pesos en aristas)")

legend("bottomleft",
       legend = c("Ecuador", "Internacional", "UCC"),
       pch    = c(15, 19, 15),
       col    = c("darkgreen", "steelblue", "red"),
       pt.cex = 1.6, bty = "n")




#  5) (Opcional) Tablas limpias ECUATORIANAS E INTERNACIONALES  ==
top6_ecu %>% select(Universidad = name, Grado = degree)
top6_int %>% select(Universidad = name, Grado = degree)




### GRADO TOTAL UNIVERSIDAD CATOLICA DE CUENCA  


library(igraph)
library(dplyr)

ucc_name <- "Universidad Católica de Cuenca"


# 1) Grados y rankings

# Data frame de grados
df_deg <- data.frame(
  name    = V(grafo)$name,
  country = V(grafo)$country,
  degree  = as.integer(degree(grafo)),
  stringsAsFactors = FALSE
)

# Grado total de la UCC
ucc_row <- df_deg %>% filter(name == ucc_name)
if (nrow(ucc_row) == 0) stop("No se encontró la UCC en el grafo.")

# Grado con ecuatorianas e internacionales (solo vecinos)
ucc_vid   <- V(grafo)[name == ucc_name]
nbrs_vids <- neighbors(grafo, ucc_vid, mode = "all")

nbrs_df <- data.frame(
  name    = V(grafo)[nbrs_vids]$name,
  country = V(grafo)[nbrs_vids]$country,
  stringsAsFactors = FALSE
)

grado_total <- length(nbrs_vids)
grado_ecu   <- sum(nbrs_df$country == "Ecuador", na.rm = TRUE)
grado_int   <- sum(nbrs_df$country != "Ecuador" & !is.na(nbrs_df$country))

# Ranking por grado (global y solo Ecuador)
rank_global <- df_deg %>% arrange(desc(degree)) %>%
  mutate(rank = row_number()) %>%
  filter(name == ucc_name) %>% select(rank) %>% as.integer()

rank_ecu_tbl <- df_deg %>% filter(country == "Ecuador") %>%
  arrange(desc(degree)) %>% mutate(rank = row_number())
rank_ecu     <- rank_ecu_tbl %>% filter(name == ucc_name) %>% pull(rank)
total_ecu    <- nrow(rank_ecu_tbl)

cat("\n--- Resumen UCC ---\n")
cat("Grado total: ", grado_total, "\n")
cat("   • Con ecuatorianas:  ", grado_ecu, "\n")
cat("   • Con internacionales:", grado_int, "\n")
cat("Ranking global por grado: ", rank_global, " / ", nrow(df_deg), "\n", sep = "")
cat("Ranking en Ecuador:       ", rank_ecu, " / ", total_ecu, "\n\n", sep = "")


####

# Calcular grados de todos los nodos
deg <- degree(grafo)

# Grado promedio de toda la red
grado_prom <- mean(deg)
cat("Grado promedio de la red:", round(grado_prom, 2), "\n\n")

# --- Top 6 Ecuador ---
ecu_nodes <- V(grafo)[V(grafo)$country == "Ecuador"]
top6_ecu <- sort(deg[ecu_nodes], decreasing = TRUE)[1:6]
cat("Top 6 universidades de Ecuador por grado:\n")
print(top6_ecu)
cat("\n")

# --- Top 6 Internacionales ---
int_nodes <- V(grafo)[V(grafo)$country != "Ecuador" & !is.na(V(grafo)$country)]
top6_int <- sort(deg[int_nodes], decreasing = TRUE)[1:6]
cat("Top 6 universidades internacionales por grado:\n")
print(top6_int)

