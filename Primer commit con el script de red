       replacement = "Universidad de las Artes"),
  list(pattern = "Metropolitana\\s+del\\s+Ecuador(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad Metropolitana del Ecuador"),
  list(pattern = "(Pontificia\\s+Univ.*Cat[oó]lica.*Ecuador(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|posgrado|comisi[oó]n).*)?$)|(^PUCE.*$)",
    replacement = "Pontificia Universidad Católica del Ecuador"),
  list(pattern = "(Escuela\\s+Polit[eé]cnica\\s+Nacional(\\s*(sede|campus|facultad|carrera|posgrado|comisi[oó]n).*)?$)|(^EPN.*$)|(National\\s+Polytechnic\\s+School.*$)",
    replacement = "Escuela Politécnica Nacional")
  
  
)



for (case in ecuador_cases) {
  data_ready1 <- data_ready1 %>%
    mutate(
      normalized_university = ifelse(
        str_detect(university, case$pattern) & country == "Ecuador",
        case$replacement,
        normalized_university
      )
    )
}

