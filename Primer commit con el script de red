##### GRAFO DE FIGURA 6 #####


library(dplyr)
library(ggplot2)
library(forcats)
library(scales)

# Usa la tabla que cargaste (ajusta el nombre si es distinto)
df_ec <- get0("tabla_ecuador", ifnotfound = NULL)
if (is.null(df_ec)) df_ec <- get0("tabla_universidades", ifnotfound = NULL)
stopifnot(!is.null(df_ec))

topN <- 10

# Top por DEGREE (solo Ecuador)
top_degree <- df_ec %>%
  filter(country == "Ecuador", !is.na(university), !is.na(degree)) %>%
  arrange(desc(degree)) %>%
  slice_head(n = topN)

# Paleta (un color por universidad)
pal <- setNames(hue_pal()(nrow(top_degree)), top_degree$university)

# --- Barras VERTICALES (con etiqueta al final) ---
p_vert <- ggplot(
  top_degree,
  aes(x = fct_reorder(university, degree), y = degree, fill = university)
) +
  geom_col(width = 0.8, color = "grey25") +
  geom_text(aes(label = comma(degree)),
            vjust = -0.25, size = 3.8, color = "black") +
  scale_fill_manual(values = pal, guide = "none") +
  scale_y_continuous(labels = comma) +
  coord_cartesian(ylim = c(0, max(top_degree$degree) * 1.12)) +
  labs(
    title = "Top-10 universidades ecuatorianas por publicaciones",
    subtitle = "Métrica: degree",
    x = "Universidad", y = "N° de artículos (degree)"
  ) +
  theme_minimal(base_size = 12) +
  theme(axis.text.x = element_text(angle = 45, hjust = 1),
        panel.grid.major.x = element_blank())

p_vert

####




##### GRAFO DE FIGURA 7 #####


library(dplyr)
library(tidyr)
library(purrr)
library(forcats)
library(ggplot2)

# 1) Detectar columna ID del artículo
posibles_ids <- c("EID","eid","SCOPUS_ID","scopus_id","ID","Id",
                  "Title","title","Titulo_Articulo","titulo_articulo")
id_col <- intersect(posibles_ids, names(data_ready1))
if (length(id_col) == 0) stop("No encuentro columna de ID del artículo (EID/Title).")
id_col <- id_col[1]   # tomamos la primera coincidencia

# 2) Construir pares universidad–universidad dentro de Ecuador
pairs_ec <- data_ready1 %>%
  filter(country == "Ecuador", !is.na(university), !is.na(.data[[id_col]])) %>%
  distinct(.data[[id_col]], university) %>%               # 1 vez por paper y universidad
  group_by(.data[[id_col]]) %>%
  summarise(unis = list(sort(unique(university))), .groups = "drop") %>%
  mutate(pairs = map(unis, ~ {
    u <- .x
    if (length(u) < 2) list() else asplit(combn(u, 2), 2)
  })) %>%
  select(-unis) %>%
  unnest(pairs) %>%
  mutate(pair = map_chr(pairs, ~ paste(.x, collapse = " ↔ "))) %>%
  count(pair, name = "freq", sort = TRUE)

if (nrow(pairs_ec) == 0) {
  message("No hay pares nacionales (todos los artículos tienen una sola universidad ecuatoriana).")
} else {
  # 3) Top-N (20) y gráfico
  top_n <- 20
  top_pairs_ec <- pairs_ec %>% slice_max(order_by = freq, n = top_n)
  
  ggplot(top_pairs_ec, aes(x = fct_reorder(pair, freq), y = freq)) +
    geom_segment(aes(xend = pair, y = 0, yend = freq), color = "grey60") +
    geom_point(size = 3.2, color = "darkgreen") +
    geom_text(aes(label = freq), hjust = -0.2, size = 3) +
    coord_flip() +
    expand_limits(y = max(top_pairs_ec$freq) * 1.10) +
    labs(title = glue::glue("Top-{top_n} pares nacionales por publicaciones compartidas"),
         subtitle = "Subred Ecuador↔Ecuador · Scopus 1920–2024",
         x = NULL, y = "Número de publicaciones (freq)") +
    theme_minimal(base_size = 12)
}



####




##### GRAFO DE FIGURA 8 ####
library(dplyr)
library(ggplot2)
library(scales)

# 1) Filtrado sano
df <- tabla_ecuador %>%
  mutate(
    degree   = as.numeric(degree),
    strength = as.numeric(strength)
  ) %>%
  filter(!is.na(degree), !is.na(strength), degree > 0, strength > 0)

N <- nrow(df)

ggplot(df, aes(x = reorder(university, -degree))) +
  # Barras (degree)
  geom_col(aes(y = degree), fill = "steelblue", alpha = 0.7, width = 0.7) +
  # Etiqueta de degree SIEMPRE visible (negra)
  geom_text(
    aes(y = degree, label = comma(degree)),
    hjust = -0.1, color = "black", size = 2.9
  ) +
  # Línea y puntos (strength)
  geom_line(aes(y = strength, group = 1), color = "red", linetype = "dashed", size = 1) +
  geom_point(aes(y = strength), color = "red", size = 2) +
  # Etiquetas de strength
  geom_text(
    aes(y = strength, label = comma(strength)),
    hjust = -0.1, size = 3, color = "black"
  ) +
  # Escala log10 con límite inferior = 1 para que NUNCA se corten las barras pequeñas
  scale_y_log10(
    name   = "Degree / Strength (escala log10)",
    limits = c(1, NA),                                   # <- clave
    breaks = c(1, 10, 100, 1000, 10000, 100000),
    labels = comma
  ) +
  coord_flip(clip = "off") +
  labs(
    title    = "Degree y Strength en escala logarítmica",
    subtitle = paste("Universidades incluidas:", N),
    x        = "Universidad"
  ) +
  theme_minimal(base_size = 10) +
  theme(
    plot.margin = margin(10, 70, 10, 10) # espacio para etiquetas a la derecha
  )



