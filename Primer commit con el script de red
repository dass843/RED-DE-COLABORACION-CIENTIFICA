# 1. Casos de Ecuador
ecuador_cases <- list(
  list(pattern = "San\\s*Francisco\\s*de\\s*Quito(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad San Francisco de Quito"),
  list(pattern = "Internacional\\s+del\\s+Ecuador(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad Internacional del Ecuador"),
  list(pattern = "Universidad\\s+de\\s+Cuenca(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad de Cuenca"),
  list(pattern = "Universidad\\s+de\\s+Las\\s+Am[eé]ricas(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad de Las Américas"),
  list(pattern = "Universidad\\s+N(ational|acional)\\s+de\\s+Loja(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad Nacional de Loja"),
  list(pattern = "Universidad(e)?\\s+Andina\\s+Sim[oó]n\\s+Bol[ií]var(,?\\s*Sede\\s*Ecuador)?(\\s*(sede|campus|matriz|extensi[oó]n|carrera|facultad|comisi[oó]n|posgrado).*)?$",
       replacement = "Universidad Andina Simón Bolívar, Sede Ecuador"),
